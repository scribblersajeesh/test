pipeline {
environment {
registry = "sajeesh83/test"
registryCredential = 'sajeesh83'
dockerImage = ''
}
agent any
stages {
stage('Cloning our Git') {
steps {
git 'https://github.com/scribblersajeesh/test.git'
}
}
stage('Building our image') {
    steps {
        sh 'docker build -t ubi9:testnov16 --no-cache .'
    }
}
stage('Deploy our image') {
    steps {
        sh "docker image push sajeesh83/test:123456"
    }
}
}
}

